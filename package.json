{
  "name": "@curvenote-themes/landing",
  "version": "1.0.2",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "postinstall": "patch-package",
    "compile": "tsc --noEmit --skipLibCheck",
    "build:css": "tailwindcss -m -i ./styles/app.css -o app/styles/app.css",
    "dev:copy": "cp remix.config.dev.js remix.config.js",
    "prod:copy": "cp remix.config.prod.js remix.config.js",
    "clean": "rimraf public/build build api",
    "build:thebe": "copy-thebe-assets public",
    "prod:build": "npm run prod:copy && npm run build:thebe && npm run build:css && remix build",
    "dev:css": "tailwindcss -w -i ./styles/app.css -o app/styles/app.css",
    "dev": "npm run dev:copy && npm run build:thebe && concurrently \"npm run dev:css\" \"remix dev\"",
    "start": "npm run build:css && remix dev"
  },
  "dependencies": {
    "@changesets/cli": "^2.27.6",
    "@curvenote/footers": "~0.1.41",
    "@curvenote/social": "~0.1.41",
    "@myst-theme/common": "^0.9.8",
    "@myst-theme/icons": "^0.9.8",
    "@myst-theme/jupyter": "^0.9.8",
    "@myst-theme/providers": "^0.9.8",
    "@myst-theme/site": "^0.9.8",
    "@myst-theme/styles": "^0.9.8",
    "@remix-run/node": "~1.17.0",
    "@remix-run/react": "~1.17.0",
    "@remix-run/vercel": "~1.17.0",
    "@scienceicons/react": "~0.0.11",
    "clsx": "^2.1.1",
    "lucide-react": "^0.400.0",
    "myst-common": "^1.4.7",
    "myst-config": "^1.4.7",
    "myst-to-react": "^0.9.8",
    "node-fetch": "^2.7.0",
    "p-limit": "^4.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tailwind-merge": "^2.3.0"
  },
  "devDependencies": {
    "@remix-run/dev": "~1.17.0",
    "@remix-run/eslint-config": "~1.17.0",
    "@remix-run/serve": "~1.17.0",
    "@tailwindcss/typography": "^0.5.9",
    "@types/node-fetch": "^2.6.4",
    "@types/react": "^18.2.24",
    "@types/react-dom": "^18.2.8",
    "@vercel/node": "^2.15.1",
    "concurrently": "^8.2.2",
    "eslint-config-curvenote": "^0.0.4",
    "npm-run-all": "^4.1.5",
    "patch-package": "^8.0.0"
  },
  "engines": {
    "node": ">=14"
  },
  "packageManager": "npm@8.10.0"
}
